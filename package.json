{
  "name": "talks",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.23.0",
  "scripts": {
    "dev": "pnpm run picker dev --open",
    "build": "rimraf dist && pnpm run picker build",
    "build:all": "rimraf dist && pnpm -r run build",
    "export": "pnpm run picker export",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "picker": "PICKER_EDITOR=code esno scripts/picker.ts",
    "prepare": "git submodule update --init --recursive"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@eslint/config-inspector": "^1.4.1",
    "@iconify/json": "^2.2.226",
    "@slidev/cli": "^52.9.1",
    "@slidev/client": "^52.9.1",
    "@slidev/theme-default": "^0.25.0",
    "@types/node": "^24.10.1",
    "@types/prompts": "^2.4.9",
    "@types/seedrandom": "^3.0.5",
    "eslint": "^9.17.0",
    "eslint-plugin-format": "^1.0.2",
    "esno": "^4.8.0",
    "execa": "^9.5.2",
    "fast-glob": "^3.3.2",
    "markdown-it-magic-link": "^0.1.4",
    "pnpm": "^10.23.0",
    "prettier-plugin-slidev": "^1.0.5",
    "prompts": "^2.4.2",
    "rimraf": "^6.0.1",
    "seedrandom": "^3.0.5",
    "taze": "^19.9.0",
    "typescript": "^5.5.3",
    "unocss": "^66.5.9",
    "vue-router": "^4.4.0",
    "vue-tsc": "^3.1.4"
  }
}
